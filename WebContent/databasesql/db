create table signUp(name varchar2(20)not null,
email varchar2(20)unique not null,
password varchar2(20)not null,
conpassword varchar2(20),
gender varchar2(20)not null,
security varchar2(30),
contect varchar2(20)
);
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

create table bookinguser(name varchar2(20),
busname varchar2(20),
email varchar2(20),
contact number(15),
gender varchar2(20),
source varchar2(15),
destination varchar2(15),
jounerydate timestamp,
noofperson number(10));

inseret into bookingsuser(name,email,contact,gender,destination,arrivaltime,departuretime,noofperson) values("satish parmer",'satish@gmail.com',9685109896,male)

