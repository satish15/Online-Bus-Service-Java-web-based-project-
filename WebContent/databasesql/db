create table bussearch(id number(10),
JOURNEY_DATE date,
busname varchar2(20)NOT NULL,
source varchar2(30)NOT NULL,
destination varchar2(30)NOT NULL,
arrival varchar2(20) ,
departure varchar2(20) ,
totalkm number(15)NOT NULL);


